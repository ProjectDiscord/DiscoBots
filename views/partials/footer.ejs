<footer class="footer pt-5 pb-4 pb-lg-5 mt-2 mt-md-0">
  <div class="container pt-lg-4">
    <div class="row pb-5">
      <div class="col-lg-4 col-md-6">
        <div class="navbar-brand text-dark p-0 me-0 mb-3 mb-lg-4">
          <img src="<%= bot.guilds.cache.get(config.server.id)?.iconURL() ||
          "https://cdn.discordapp.com/embed/avatars/0.png" %>"
          type="image/x-icon class="me-2" width="42"><%= config.website.name %>
        </div>
        <p class="fs-sm pb-lg-3 mb-4"><%= config.website.description %></p>

        <form class="needs-validation">
          <label for="subscr-email" class="form-label">Send us a message</label>
          <div class="input-group">
            <input
              type="text"
              id="subscr-email"
              placeholder="Enter a message.."
              class="form-control rounded-start ps-5"
              required="" />
            <i
              class="bx bx-envelope fs-lg text-muted position-absolute top-50 start-0 translate-middle-y ms-3 zindex-5"></i>
            <button type="submit" class="btn btn-primary">Send</button>
          </div>
        </form>

        <script>
          document.addEventListener('DOMContentLoaded', function() {
            const form = document.querySelector('.needs-validation');
            const input = document.getElementById('subscr-email');

            form.addEventListener('submit', function(event) {
              event.preventDefault();

              const message = input.value.trim();

              if (message === '') {
                return;
              }

              const emailAddress = "<%= config.website.email %>";
              const subject = 'Message from website';

              const mailtoLink = `mailto:${emailAddress}?subject=${encodeURIComponent(subject)}&body=${encodeURIComponent(message)}`;

              window.location.href = mailtoLink;

              form.reset();
            });
          });
        </script>
      </div>
